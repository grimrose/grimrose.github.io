  "bugs": {
    "url": "https://github.com/grimrose/grimrose.github.io/issues"
  },
  "homepage": "https://github.com/grimrose/grimrose.github.io#readme",
  "dependencies": {
    "bulma": "^0.1.0",
    "font-awesome": "^4.6.3",
    "highlight.js": "^9.5.0",
    "mithril": "^0.2.5",
    "sass-loader": "^4.0.0"
  },
  "devDependencies": {
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "del-cli": "^0.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome-webpack": "0.0.4",
    "gh-pages": "^0.11.0",
    "less": "^2.7.1",
